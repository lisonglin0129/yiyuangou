.edit_user_password{
    margin:0 20px;
    cursor: pointer;
}
.modal.password_edit{
    position: fixed;
    height: 100%;
    width:100%;
    z-index: 999;
    left:0;
    top:0;
    display: none;
}
.modal_bg{
    background: rgba(0,0,0,0.5);
    +background:#aaa;
    position: absolute;
    z-index: 1;
    top:0;
    left:0;
    width:100%;
    height: 100%;
}
.modal_main{
    width:540px;
    position: absolute;
    left:50%;
    top: 50%;
    display: block;
    -webkit-transform: translate(-50%, -50%);
    -moz-transform: translate(-50%, -50%);
    -ms-transform: translate(-50%, -50%);
    -o-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background: #fff;
    z-index: 2;
    border-radius: 5px;
    box-sizing: border-box;
    padding:20px 35px;
    box-shadow: 0 0 2px #fff;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
    -ms-transition: all 0.3s;
    -o-transition: all 0.3s;
    transition: all 0.3s;
}
.pwd_form{
    overflow: hidden;
    width: 100%;
    margin:32px auto 0;
}
.modal_con{
    width:100%;
}
.modal_con>h2{
    line-height: 68px;
    text-align: center;
    font-weight: 300;
    font-size: 32px;
    color:#ee174e;
    display: block;
    width:100%;
    letter-spacing: 3px;
    border-bottom:1px solid #aaa;
}
.password_edit .modal_close{
    display: block;
    width:36px;
    height: 36px;
    background: rgba(0,0,0,0.6);
    border-radius: 100%;
    text-align: center;
    line-height: 36px;
    color:#fff;
    position: absolute;
    top:-20px;
    right:-20px;
    cursor: pointer;
}
.password_edit .modal_close>i{
    font-size:22px;
    color:#fff;

}
.password_edit  .input_line{
    height: 46px;
    margin-bottom:30px;
    display: block;
    position: relative;
}
.password_edit .input_line:after{
    height: 0;
    content:'';
    display: block;
    clear: both;
    visibility: hidden;
}
.password_edit  .input_line>label{
    float: left;
    position: relative;
    width:120px;
    line-height: 46px;
    height: 46px;
    padding-left:20px;
    font-size: 16px;
    font-weight: 300;
    letter-spacing: 1px;
}
.password_edit  .input_line>label>span{
    position: absolute;
    left:6px;
    color:#ff0000;
    font-size: 20px;
    top:5px;
}
.password_edit  .input_line>input{
    width:310px;
    float: left;
    border:1px solid #d2d2d2;
    line-height: 44px;
    height:46px;
    padding-left:10px;
    font-size: 14px;
    outline: none;
    font-weight: 300;
    box-sizing: border-box;
}
.password_edit  .input_line>span{
    position: absolute;
    line-height: 20px;
    top:48px;
    font-size: 12px;
    left:140px;
    padding-left:20px;
    color:#ff0000;
}
.password_edit .input_line>span>i{
    position: absolute;
    left:0;
    line-height: 20px;
}
.password_edit .pwd_submit{
    width:300px;
    height: 46px;
    display: block;
    text-align: center;
    letter-spacing: 2px;
    line-height: 46px;
    background: #ee174e;
    color:#fff;
    outline: none;
    font-size: 18px;
    margin:60px auto 10px;
    border:none;
    cursor: pointer;
    -webkit-transition: all 0.3s;
    -moz-transition: all 0.3s;
    -ms-transition: all 0.3s;
    -o-transition: all 0.3s;
    transition: all 0.3s;
}
.password_edit .pwd_submit:hover{
    background: #CE1342;
}
.password_edit .pw_edit_success{
    display: none;
    width:100%;
    height: 340px;
}
