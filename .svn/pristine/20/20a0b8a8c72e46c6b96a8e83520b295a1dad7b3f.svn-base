/*分页*/

#page{
    display: inline-block;
    padding-left: 0;
    margin: 20px 0;
    border-radius: 4px;
}
#page>a{
    position: relative;
    float: left;
    padding: 6px 12px;
    margin-left: -1px;
    text-decoration: none;
    background-color: #fff;
    border: 1px solid #ddd;

}
#page>a.cur{
    cursor: default;
    background-color: #6faed9;
    border-color: #6faed9;
    color: #fff;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
}
#page>p{
    position: relative;
    float: left;
    padding: 6px 12px;
    margin-left: -1px;
    background-color: #eee;
    border: 1px solid #ddd;
    color:#999;

}
#page>a{
    cursor: pointer;
}
#page>a:hover{background: #eaeff2;}
#page p.pageRemark{
    border:none;
    margin:0 10px;
    background: #fff;
}
#page p.pageRemark b{
    font-size:12px;
    margin:0 5px;
    color :#428bca;
    font-weight:bold;
}

.navbar-container:after,
.main-container-inner:after,
.page-content:after
{
    height:0;
    display: block;
    visibility: hidden;
    content:'';
    clear: both;
}
.m-footer{
    width:100%;
    position: fixed;
    bottom: 0;
    box-sizing: border-box;
    z-index: 999;
    /*padding-left:190px;*/
}
.m-copyright{
    float: left;
    width: 100%;
    text-align: center;
    background: #eee;
    padding: 10px;
    z-index: 10;
    box-shadow: 0 0 3px #999;
}
.m-copyright .g-wrap:after{
    display: block;
    visibility: hidden;
    height: 0;
    content: " ";
    clear:both;
}
.m-copyright .g-wrap{
    width:1000px;
    margin:auto;
}
.m-copyright-logo{
    float: left;
    width:500px;
    height: 50px;
}
.m-copyright-logo a{
    margin:0 10px;
}
.m-copyright-txt{
    width:400px;
    float: left;
    height: 50px;
    text-align: left;
    padding-left:10px;
    line-height: 20px;
    box-sizing: border-box;
}
.m-copyright-txt p{
    margin-top:20px;
}

