}
.ras{
	position: absolute;
	z-index: 999;
	top: 2px;
	left: 0px;
	width:50px;
	height: 50px;
    opacity:0;
	z-index:1000;
	border:1px solid #666;
}
.ras1{
	position: absolute;
	top: 8px;
	left: 0px;
	height: 30px;
	width: 30px;
	border-radius: 50%;
	background-color: #fff;
	border:1px solid #666;
}
.ras:checked + label{
	top: 5px;
	border:none;
	width:38px;
	height: 38px;
	background:url(../img/ds.png) no-repeat -3px -3px #fff;
	background-size: 44px 44px;
}
