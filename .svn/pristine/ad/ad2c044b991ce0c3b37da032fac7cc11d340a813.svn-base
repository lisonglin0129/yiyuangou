{extend name="base/base"/}
{block name="title"}我的推广{/block}
{block name="user_css"}
{css href="__static__/css/usercenter.css" /}
{css href="__static__/css/promote.css" /}
{/block}
{block name='content'}
<div class="g-body">
    <div class="m-user">
        <div class="g-wrap">
            <div class="m-user-frame-wraper">
                <!--用户中心  菜单-->
                {include file="ucenter/_nav" /}
                <!--用户中心 main-->
                <div class="m-user-frame-colMain ">
                    <div class="m-user-frame-content" >
                        <ul class="w-crumbs f-clear">
                            <li class="w-crumbs-item">当前位置：</li>
                            <li class="w-crumbs-item"><a href="promote.html">我的推广</a><span class="w-crumbs-split">&gt;</span></li>
                            <li class="w-crumbs-item w-crumbs-active">提现</li>
                        </ul>
                        <div class="m-user-comm-wraper">
                            <div class=" pro-title">
                                <h2>提现账户</h2>
                                <div class="pro-extract_btns">
                                    <a href="{:U('extract_submit')}" class="pro-e-add-btn">立即提现</a>
                                    <a href="{:U('extract_list')}" class="pro-e-list-btn">提现记录</a>
                                </div>
                            </div>
                            <div class=" pro-detail pex-con">


                               {notempty name="accounts"}

                                <div class="pec-accounts">
                                    {volist name="accounts" id="vo"}
                                    {if condition="$vo['account_type'] eq 2"}
                                    <div class="pec-banks">
                                        <i class="iconfont icon-zhifubao"></i>
                                        <div class="pec-b-text">
                                            <h2>支付宝<span>网络银行</span></h2>
                                            <p>****&nbsp;****&nbsp;****&nbsp;{:substr($vo['bank_no'],-4)}</p>
                                        </div>
                                        <span class="pec-b-del" aid="{$vo.id}">
                                            <i class="iconfont">&#xe65b;</i>
                                        </span>
                                    </div>
                                    {else /}
                                    <div class="pec-banks">
                                        <i class="iconfont icon-{switch name='vo.type'}
                                    {case value='3'}zhonghang{/case}
                                    {case value='4'}nonghang{/case}
                                     {case value='5'}gongshang{/case}
                                    {case value='6'}jianshen{/case}
                                     {case value='7'}minsheng{/case}
                                    {case value='8'}jiaohang{/case}
                                     {case value='9'}guangfa{/case}
                                    {case value='10'}xingye{/case}
                                     {case value='11'}zhaoshang{/case}
                                    {case value='12'}guangda{/case}
                                     {case value='13'}youzheng{/case}
                                    {case value='14'}zhongxin{/case}
                                     {case value='15'}pufa{/case}
                                    {/switch}
                                    "></i>
                                    <div class="pec-b-text">
                                    <h2>{$vo.bank_name}<span>储蓄卡</span></h2>
                                    <p>****&nbsp;****&nbsp;****&nbsp;{:substr($vo['bank_no'],-4)}</p>
                                    </div>
                                    <span class="pec-b-del" aid="{$vo.id}">
                                    <i class="iconfont">&#xe65b;</i>
                                    </span>
                                    </div>
                                    {/if}
                                    {/volist}
                                    {if condition="count($accounts) lt 8"}
                                    <a href="{:U('add_extract_account')}" class="pec-banks-add">
                                        <h6>
                                            <i class="iconfont">&#xe65c;</i>
                                            <span>添加账户</span>
                                        </h6>
                                    </a>
                                    {/if}
                                </div>
                                <div class="pec-notice">
                                    <span><i class="iconfont pec-n-ti">&#xe64c;</i></span>
                                    <div class="pec-n-t">
                                        <h4>注意事项</h4>
                                        <p>
                                            1、跨行转账，款项转移以后一般需要1-5个工作日到账，请耐心等待。<br>
                                            2、提现成功以后，实际需要1-2个工作日内到账。
                                        </p>
                                    </div>
                                </div>
                               {else /}
                                <div class="pec-account-no">
                                    <i class="iconfont">&#xe649;</i>
                                    <h4>当前没有账号,请先添加!</h4>
                                    <a class="pec-account-add" href="{:U('add_extract_account')}"><i class="iconfont">&#xe64a;</i><span>添加账号</span></a>
                                </div>
                                {/notempty}

                            </div>
                        </div>
                    </div>

                </div>
                <div class="m-user-frame-clear"></div>
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="del_url" value="{:U('del_account')}">
{/block}
{block name="user_js"}
{js href="__static__/js/ucenter/extract.js" /}
{/block}