.m-win-vg-line{
    width:800px;
    margin:60px auto;
    line-height: 20px;
}
.m-win-vg-line:after{
    visibility: hidden;
    height: 0;
    display: block;
    content: " ";
    clear: both;
}
.m-win-vg-line>div{
    float: left;
    width:14px;
    height:14px;
    border-radius: 100%;
    background: #bdb8b8;
    margin:0 15px;
    position: relative;
}
.m-win-vg-line>div>h6{
    width:200px;
    display: block;
