button{
	position: relative;
	background:none;
	border:none;
	width: 750px;
	height: 70px;
	color: #fff;
	font-size: 28px;
}
.det-dates{
	position: absolute;
    z-index: 9;
    top: 161px;
    left:4px;
    width: 740px;
    height: auto;
    display: none;
}

.ico11{
	margin-left: 10px;color: #fff;
}
.arrow-up {
    position: absolute;
    top: 3px;
    left: 50%;
    margin-left: -6px;
    width: 0;
    height: 0;
    border-left: 8px solid transparent;
    border-right: 8px solid transparent;
    border-bottom: 13px solid #fff;
}
.arrow-ups{
	 z-index: -5;
	 top: 1px;
	 border-bottom: 13px solid  #e7e7e7;
}
.det-dates1{
	width: 100%;
	height: 300px;
	border:1px solid #e7e7e7;
	margin-top: 13px;
	color: #999;
	border-radius: 2px;
	background-color: #fff;
}
.det-dates2{
	margin-top: 10px;
	height: auto;
	background-color: #fff;
	display: none;
}
.det-dates1 ul li{
	font-size: 28px;
	width: 100%;
	height: 59px;
	line-height: 59px;
	text-align: center;
	border-bottom: 1px solid #e7e7e7;
}
.det-dates1 ul li:active{
	background-color: #fd5e64;
	color: #fff;
}

