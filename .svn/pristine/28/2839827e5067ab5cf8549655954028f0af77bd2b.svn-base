-- Table structure for sp_users
-- ----------------------------
DROP TABLE IF EXISTS `sp_users`;
CREATE TABLE `sp_users` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `username` varchar(255) CHARACTER SET utf8 COLLATE utf8_bin DEFAULT NULL COMMENT '登录用户名',
  `password` varchar(50) DEFAULT NULL COMMENT '密码',
  `nick_name` varchar(255) DEFAULT NULL COMMENT '昵称',
  `role_list` text COMMENT '角色列表',
  `origin` varchar(100) DEFAULT NULL COMMENT '来源',
  `type` tinyint(3) DEFAULT '1' COMMENT '用户类型 -1.rt人 1普通用户2商户',
  `status` tinyint(5) DEFAULT '1' COMMENT '状态 1正常 -1删除 -2禁用',
  `user_pic` int(11) DEFAULT '1' COMMENT '用户头像',
  `age` tinyint(5) DEFAULT NULL COMMENT '年龄',
  `sex` varchar(10) DEFAULT 'man' COMMENT '用户性别',
  `money` decimal(11,2) DEFAULT '0.00' COMMENT '金钱/香肠',
  `cash` decimal(11,2) DEFAULT NULL COMMENT '现金,可提现金额',
  `score` int(11) DEFAULT '0' COMMENT '积分',
  `real_name` varchar(50) DEFAULT NULL COMMENT '真实姓名',
  `phone` varchar(15) DEFAULT NULL COMMENT '手机号码',
  `phone_area` varchar(255) DEFAULT NULL COMMENT '手机归属地',
  `email` varchar(100) DEFAULT NULL COMMENT '电子邮箱',
  `qq` varchar(50) DEFAULT NULL COMMENT 'QQ号码',
  `reg_ip` varchar(20) DEFAULT NULL COMMENT '注册ip',
  `ip_area` varchar(255) DEFAULT NULL COMMENT 'IP归属地',
  `zip_code` int(6) DEFAULT NULL COMMENT '邮编',
  `birthday` date DEFAULT NULL COMMENT '生日',
  `constellation` varchar(0) DEFAULT NULL COMMENT '星座',
  `promoter` varchar(50) DEFAULT NULL COMMENT '邀请人',
  `last_login_ip` varchar(20) DEFAULT NULL COMMENT '最后登录ip',
  `is_pay` enum('true','false') DEFAULT 'false' COMMENT '是否是付费用户',
  `is_test` enum('false','true') DEFAULT NULL COMMENT '是否为测试用户',
  `user_group` tinyint(255) unsigned DEFAULT '0' COMMENT '用户组：1为后台管理员',
  `receive_sms` enum('false','true') DEFAULT 'true' COMMENT '接受短信提示',
  `update_time` int(11) DEFAULT NULL COMMENT '修改时间',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  `weibo_id` varchar(50) DEFAULT NULL,
  `qq_id` varchar(50) DEFAULT NULL,
  `weixin_id` varchar(50) DEFAULT NULL,
  `wx_mp_openid` char(28) DEFAULT NULL COMMENT '微信公众平台openid',
  `shop_id` int(11) unsigned DEFAULT '0' COMMENT '商家id',
  `spread_userid` int(11) DEFAULT '0' COMMENT '推广的用户id',
  `promote_origin` int(11) DEFAULT '1' COMMENT '注册来源ID 对应origin表',
  PRIMARY KEY (`id`),
  UNIQUE KEY `用户索引` (`id`) USING BTREE,
  UNIQUE KEY `用户名` (`username`),
  KEY `手机号` (`phone`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=10000 DEFAULT CHARSET=utf8 COMMENT='用户表';

-- ----------------------------
-- Records of sp_users
-- ----------------------------

