-- Table structure for sp_file_list
-- ----------------------------
DROP TABLE IF EXISTS `sp_file_list`;
CREATE TABLE `sp_file_list` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `name` varchar(255) DEFAULT NULL COMMENT '原文件名称',
  `uid` int(11) DEFAULT NULL COMMENT '所属用户',
  `status` enum('-2','-1','1') DEFAULT NULL COMMENT '-2禁用 -1. 删除 1正常 ',
  `type` varchar(255) DEFAULT NULL COMMENT '文件头类型',
  `path` varchar(255) DEFAULT NULL COMMENT '附件地址',
  `size` int(11) DEFAULT NULL COMMENT '大小',
  `ext` varchar(15) DEFAULT NULL COMMENT '扩展名',
  `hash` varchar(255) DEFAULT NULL COMMENT '文件哈希',
  `sha1` varchar(255) DEFAULT NULL COMMENT '校验码',
  `qiniu_path` varchar(255) DEFAULT NULL COMMENT '七牛地址',
  `save_name` varchar(255) DEFAULT NULL COMMENT '保存名称',
  `save_path` varchar(255) DEFAULT NULL COMMENT '保存位置',
  `update_time` int(11) DEFAULT NULL COMMENT '更新时间',
  `create_time` int(11) DEFAULT NULL COMMENT '保存时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='上传文件列表';

-- ----------------------------
-- Records of sp_file_list
-- ----------------------------

-- ----------------------------
-- Table structure for sp_goods
-- ----------------------------
DROP TABLE IF EXISTS `sp_goods`;
CREATE TABLE `sp_goods` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `status` tinyint(2) DEFAULT '1' COMMENT '1正常 -1删除 -2禁用',
  `name` varchar(255) DEFAULT NULL COMMENT '商品标题',
  `sub_title` varchar(255) DEFAULT NULL COMMENT '副标题',
  `buy_times` int(11) DEFAULT '0' COMMENT '总购买人数',
  `category` int(11) DEFAULT NULL COMMENT '商品分类id',
  `mid` int(11) DEFAULT NULL COMMENT '所属大分类的id',
  `deposer_type` varchar(255) DEFAULT NULL COMMENT '夺宝类型,如果选择夺宝类型,则不允许用户修改预定义值',
  `init_times` int(11) DEFAULT NULL COMMENT '初始化购买次数,如果限制每次最少购买次数,则等于最少购买次数的倍数',
  `unit_price` decimal(10,2) DEFAULT NULL COMMENT '单价(计算得到)',
  `min_times` int(11) DEFAULT NULL COMMENT '每次最少购买次数',
  `sum_times` int(11) DEFAULT NULL COMMENT '购买总次数,由总金额除单价四舍五入多一位算得',
  `max_times` int(11) unsigned DEFAULT '1000' COMMENT '限制每次最大购买次数',
  `num` int(11) DEFAULT NULL COMMENT '库存数量',
  `index_img` int(255) unsigned DEFAULT NULL COMMENT '主图',
  `pic_list` text COMMENT '缩略图列表',
  `buy_price` decimal(10,2) DEFAULT NULL COMMENT '直接购买价格',
  `price` decimal(10,2) DEFAULT NULL COMMENT '一元购价格',
  `detail` text COMMENT '商品详情',
  `remarks` varchar(255) DEFAULT NULL COMMENT '备注',
  `parameter` tinytext COMMENT '产品参数地址或json数据',
  `search_times` int(11) DEFAULT '0' COMMENT '搜索次数，用于热门搜索',
  `update_time` int(11) DEFAULT NULL COMMENT '更新时间',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  `shop_id` int(11) unsigned DEFAULT '0' COMMENT '商家id',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='商品基础信息表\r\n本表存放商品基础信息\r\n用于创建夺宝项目时选择使用';

-- ----------------------------
-- Records of sp_goods
-- ----------------------------

-- ----------------------------
-- Table structure for sp_home_carouse
-- ----------------------------
DROP TABLE IF EXISTS `sp_home_carouse`;
CREATE TABLE `sp_home_carouse` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `image_id` int(11) DEFAULT NULL COMMENT '图片地址',
  `content` tinytext COMMENT '点击图片若是链接，则是点击后的链接，若是商品id，则是id，若是搜索的关键字，则是关键字',
  `type` tinyint(1) DEFAULT NULL COMMENT '图片表示类型,1是商品，2是网页链接，3是搜索',
  `flag` tinyint(1) DEFAULT NULL COMMENT '是否显示在轮播图，1是显示，0是不显示',
  `title` varchar(255) DEFAULT NULL COMMENT '标题',
  `sort` int(10) DEFAULT NULL COMMENT '序号越大，排序越靠前',
  `platform` tinyint(11) DEFAULT '1' COMMENT '1 网页端 2手机网页端 3安卓 4 IOS',
  `mid` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=49 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='主图轮播 手机端';

