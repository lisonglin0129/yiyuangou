  `uid` int(11) unsigned DEFAULT NULL COMMENT '用户id',
  `reward` decimal(10,2) unsigned DEFAULT NULL COMMENT '收益金额',
  `create_time` int(11) unsigned DEFAULT NULL COMMENT '收益时间',
  `total` decimal(10,2) DEFAULT '0.00' COMMENT '消费金额',
  `level` tinyint(3) DEFAULT '0' COMMENT '用户等级',
  `spread_uid` int(11) DEFAULT NULL COMMENT '推广人用户id',
  `month_time` int(11) DEFAULT '0' COMMENT '月份时间',
  `status` tinyint(3) DEFAULT '2' COMMENT '发放状态1:发放2:未发放',
  `op_time` int(11) DEFAULT '0' COMMENT '操作时间：删除和发放',
  `reward_score` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '积分奖励',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=83 DEFAULT CHARSET=utf8 COMMENT='推广收益表';

