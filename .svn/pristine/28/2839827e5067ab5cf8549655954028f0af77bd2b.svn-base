DROP TABLE IF EXISTS `sp_article`;
CREATE TABLE `sp_article` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '#',
  `title` varchar(255) DEFAULT NULL COMMENT '标题',
  `name` varchar(255) DEFAULT NULL COMMENT '名字（seo)',
  `category` smallint(255) unsigned DEFAULT NULL COMMENT '分类',
  `state` tinyint(255) DEFAULT NULL,
  `outlink` varchar(255) DEFAULT NULL COMMENT '跳转链接',
  `content` text COMMENT '内容',
  `content_mob` text COMMENT '手机端显示',
  `sort` varchar(255) DEFAULT NULL COMMENT '排序',
  `view_num` int(255) unsigned DEFAULT NULL COMMENT '浏览次数',
  `vote_up` int(255) unsigned DEFAULT NULL COMMENT '赞数量',
  `vote_down` int(255) unsigned DEFAULT NULL COMMENT '踩数量',
  `create_time` int(10) unsigned DEFAULT NULL COMMENT '创建时间',
  `update_time` int(10) unsigned DEFAULT NULL COMMENT '更新时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=84 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='文章/新闻/帮助/关于';

