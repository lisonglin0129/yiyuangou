DROP TABLE IF EXISTS `log_balance`;
CREATE TABLE `log_balance` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `uid` int(11) DEFAULT NULL,
  `order_id` varchar(50) DEFAULT NULL,
  `type` enum('default','back','reduce','add') DEFAULT NULL COMMENT '记录类型 add充值 reduce扣款  back退款 default默认',
  `money` varchar(255) DEFAULT NULL,
  `desc` varchar(255) DEFAULT NULL,
  `create_time` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='余额使用日志';

