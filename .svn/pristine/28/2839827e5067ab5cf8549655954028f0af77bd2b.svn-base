-- ----------------------------
-- Table structure for admin_menu
-- ----------------------------
DROP TABLE IF EXISTS `admin_menu`;
CREATE TABLE `admin_menu` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `status` tinyint(4) DEFAULT '1' COMMENT '状态',
  `path` varchar(255) DEFAULT NULL COMMENT '菜单的路径,用于展开菜单',
  `title` varchar(50) DEFAULT '' COMMENT '菜单标题',
  `pid` int(10) DEFAULT '0' COMMENT '上级分类ID',
  `sort` int(10) unsigned DEFAULT '0' COMMENT '排序（同级有效）',
  `ico` varchar(255) DEFAULT NULL COMMENT '签名的ico小图标',
  `level` tinyint(2) DEFAULT '0' COMMENT '等级',
  `url` varchar(255) DEFAULT '' COMMENT '链接地址',
  `target` enum('blank','self') DEFAULT 'blank' COMMENT '打开方式',
  `param` varchar(255) DEFAULT NULL COMMENT 'get的参数',
  `hide` enum('false','true') DEFAULT 'false',
  `tip` varchar(255) DEFAULT '' COMMENT '提示',
  PRIMARY KEY (`id`),
  KEY `pid` (`pid`),
  KEY `status` (`status`)
) ENGINE=InnoDB AUTO_INCREMENT=78 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='菜单表';

