CREATE TABLE `sp_win_record` (
  `id` int(10) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `read_flag` tinyint(255) DEFAULT NULL COMMENT '已读标记',
  `nper_id` varchar(255) DEFAULT NULL COMMENT '期数id',
  `open_num` varchar(255) DEFAULT NULL COMMENT '开奖号码',
  `remainder` varchar(255) DEFAULT NULL COMMENT '余数',
  `luck_num` varchar(255) DEFAULT NULL COMMENT '幸运数字',
  `luck_user` varchar(255) DEFAULT NULL COMMENT '幸运用户',
  `luck_uid` int(10) DEFAULT NULL COMMENT '幸运用户uid',
  `luck_time` varchar(255) DEFAULT '0' COMMENT '幸运时间',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  `goods_id` int(10) DEFAULT NULL COMMENT '商品id',
  `notice` tinyint(1) DEFAULT '0' COMMENT '是否通知中奖，1是通知，0是尚未通知，默认为0',
  `order_id` varchar(255) DEFAULT NULL COMMENT '关联订单的号码',
  `order_num` int(11) unsigned DEFAULT NULL COMMENT '关联订单的购买数量',
