-- ----------------------------
-- Table structure for sp_coupon_cat
-- ----------------------------
DROP TABLE IF EXISTS `sp_coupon_cat`;
CREATE TABLE `sp_coupon_cat` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `platform` enum('lvdou','jiasutu') DEFAULT NULL COMMENT '优惠券平台',
  `uid` int(11) DEFAULT NULL COMMENT '创建人id',
  `title` varchar(255) DEFAULT NULL COMMENT '优惠券名称',
  `status` int(11) DEFAULT '1' COMMENT '状态:1生效',
  `bind_pak` varchar(255) DEFAULT '' COMMENT '绑定套餐id,用逗号分割:9,10  套餐为9或10的时候可用,当优惠券类型为时间充值券的时候,本字段表示充值用户套餐',
  `limit_time` varchar(255) DEFAULT '' COMMENT '限定开通时长(月): 格式:12-24,1-3',
  `limit_session` int(5) DEFAULT NULL COMMENT '终端数限制',
  `type` enum('add_time','recharge','reduce_money','discount') DEFAULT NULL COMMENT '''add_time''折扣,''recharge''减现金,''reduce_money''充值金额,''discount''充值时间',
  `value` decimal(10,1) DEFAULT '0.0' COMMENT '折扣或折现值',
  `start_time` int(11) DEFAULT '0' COMMENT '生效时间',
  `end_time` int(11) DEFAULT '0' COMMENT '失效时间',
  `content` varchar(255) DEFAULT NULL COMMENT '备注',
  `update_time` int(11) DEFAULT NULL COMMENT '更新时间',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='优惠券/红包分类';

-- ----------------------------
-- Records of sp_coupon_cat
-- ----------------------------

-- ----------------------------
-- Table structure for sp_coupon_list
-- ----------------------------
DROP TABLE IF EXISTS `sp_coupon_list`;
CREATE TABLE `sp_coupon_list` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `pid` int(11) NOT NULL COMMENT '所属优惠券批次',
  `code` varchar(255) DEFAULT NULL COMMENT '优惠券号',
  `password` varchar(255) DEFAULT '' COMMENT '充值密码',
  `status` int(11) DEFAULT '1' COMMENT '是否使用   1正常 2已领取 |-1删除 -2锁定',
  `uid` text COMMENT '使用用户id,用逗号分隔',
  `bind_user` text COMMENT '绑定用户id',
  `use_times` int(11) DEFAULT '1' COMMENT '使用次数 是否能用请根据使用次数判断,为-1是不限次数',
  `update_time` int(11) DEFAULT NULL COMMENT '使用时间',
  PRIMARY KEY (`id`),
  UNIQUE KEY `code` (`code`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='优惠券/红包详情表';

-- ----------------------------
-- Records of sp_coupon_list
-- ----------------------------

