-- Table structure for log_behavior
-- ----------------------------
DROP TABLE IF EXISTS `log_behavior`;
CREATE TABLE `log_behavior` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `uid` int(11) DEFAULT NULL COMMENT '用户id',
  `use_login` varchar(50) DEFAULT NULL COMMENT '用户名',
  `modules` varchar(50) DEFAULT NULL COMMENT '模块名称',
  `controllers` varchar(50) DEFAULT NULL COMMENT '控制器名称',
  `action` varchar(50) DEFAULT NULL COMMENT '操作名称',
  `action_cn_name` varchar(50) DEFAULT NULL COMMENT '操作中文名称',
  `request_type` enum('normal','ajax') DEFAULT 'normal' COMMENT '是否为ajax请求',
  `method_type` enum('delete','put','post','get') DEFAULT 'get' COMMENT '请求方式 1get 2post 3put 4delete',
  `request_data` text COMMENT '请求数据',
  `platform` enum('blackberrie','ios','android','others','mac','linux','windows') DEFAULT NULL COMMENT '运行平台 1win 2 命令行',
  `ip` varchar(255) DEFAULT NULL COMMENT '用户ip',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='行为日志';

