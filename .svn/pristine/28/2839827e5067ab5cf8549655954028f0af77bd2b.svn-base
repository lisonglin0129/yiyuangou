DROP TABLE IF EXISTS `sp_image_list`;
CREATE TABLE `sp_image_list` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `name` varchar(255) DEFAULT NULL COMMENT '图片名称',
  `uid` int(11) DEFAULT NULL COMMENT '上传用户id',
  `type` tinyint(20) unsigned DEFAULT NULL COMMENT '类型： 0:网站素材 1:商品图片 2:用户头像 3:用户晒单',
  `origin` varchar(20) DEFAULT NULL COMMENT '来源 0:后台 1:web 2:安卓 3:IOS',
  `status` int(11) DEFAULT NULL COMMENT '图片状态： -2 禁用 -1删除 1.正常 2.审核中',
  `img_path` text COMMENT 'img保存地址',
  `thumb_path` text COMMENT '缩略图地址',
  `width` int(11) DEFAULT NULL COMMENT '图片宽度',
  `height` int(11) DEFAULT NULL COMMENT '图片高度',
  `ext` varchar(255) DEFAULT NULL COMMENT '图片后缀',
  `hash` varchar(64) DEFAULT NULL COMMENT '图片哈希校验码',
  `sha1` varchar(165) DEFAULT NULL COMMENT 'sha1校验码',
  `qiniu_path` varchar(255) DEFAULT '' COMMENT '七牛云存储访问地址',
  `update_time` int(11) DEFAULT NULL COMMENT '图片更新时间',
  `create_time` int(11) DEFAULT NULL COMMENT '图片创建时间',
  PRIMARY KEY (`id`),
  UNIQUE KEY `id` (`id`) USING BTREE
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='图片表';

