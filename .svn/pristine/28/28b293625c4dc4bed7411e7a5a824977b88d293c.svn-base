DROP TABLE IF EXISTS `admin_role_list`;
CREATE TABLE `admin_role_list` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `status` int(11) DEFAULT '1' COMMENT '状态 -2禁用 1正常 -1删除',
  `pid` int(11) DEFAULT NULL COMMENT '父级id',
  `name` varchar(255) DEFAULT NULL COMMENT '角色名称',
  `sort` int(255) DEFAULT '100' COMMENT '排序',
  `index_id` int(11) DEFAULT NULL COMMENT '首页菜单地址id',
  `auth_list` text COMMENT '权限列表 逗号分割',
  `desc` varchar(255) DEFAULT NULL COMMENT '备注',
  `is_super` enum('false','true') DEFAULT NULL,
  `update_time` int(11) DEFAULT NULL COMMENT '更新时间',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=38 DEFAULT CHARSET=utf8 COMMENT='用户角色表';

