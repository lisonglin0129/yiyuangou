DROP TABLE IF EXISTS `sp_luck_num`;
CREATE TABLE `sp_luck_num` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `status` enum('false','true') DEFAULT 'false' COMMENT '是否获奖  true获奖 false未获奖',
  `uid` int(11) DEFAULT NULL COMMENT '用户id',
  `username` varchar(255) DEFAULT NULL COMMENT '幸运用户用户名',
  `nper_id` int(11) DEFAULT NULL COMMENT '期数id',
  `address_id` int(11) DEFAULT NULL COMMENT '获奖之后的收货地址,获奖后填写',
  `goods_id` int(11) DEFAULT NULL COMMENT '商品id',
  `order_id` varchar(50) DEFAULT NULL COMMENT '订单id',
  `num` int(11) DEFAULT NULL COMMENT '数量',
  `luck_num` varchar(50) DEFAULT NULL COMMENT '幸运号码',
  `code_list` longtext,
  `create_time` varchar(200) DEFAULT NULL COMMENT '毫秒级时间戳',
  `logistics_company` varchar(255) DEFAULT NULL COMMENT '物流公司',
  `logistics_number` varchar(255) DEFAULT NULL COMMENT '物流单号',
  `logistics_status` tinyint(255) unsigned DEFAULT '0' COMMENT '状态：1 获得商品。2 确认收获地址。3 商品派发。4 确认收货 。5 晒单分享',
  `address_data` text COMMENT 'json:确认收货地址时的快照',
  `join_type` tinyint(3) NOT NULL DEFAULT '0' COMMENT '0：非0元1：奇数参与2：偶数参与',
  PRIMARY KEY (`id`),
  KEY `order_id` (`order_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='幸运号码表,用户购买后获得的幸运号码,和订单,用户,期数,关联';

