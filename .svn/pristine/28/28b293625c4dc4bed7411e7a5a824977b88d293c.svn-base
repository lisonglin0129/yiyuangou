-- Table structure for sp_win_record
-- ----------------------------
DROP TABLE IF EXISTS `sp_win_record`;
CREATE TABLE `sp_win_record` (
  `id` int(10) NOT NULL AUTO_INCREMENT COMMENT '主键id',
  `read_flag` tinyint(5) DEFAULT NULL COMMENT '已读标记 1',
  `nper_id` varchar(255) DEFAULT NULL COMMENT '期数id',
  `open_num` varchar(255) DEFAULT NULL COMMENT '开奖号码',
  `remainder` varchar(255) DEFAULT NULL COMMENT '余数',
  `luck_num` varchar(255) DEFAULT NULL COMMENT '幸运数字',
  `luck_user` varchar(255) DEFAULT NULL COMMENT '幸运用户',
  `luck_uid` int(10) DEFAULT NULL COMMENT '幸运用户uid',
  `luck_time` varchar(255) DEFAULT '0' COMMENT '幸运时间',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  `goods_id` int(10) DEFAULT NULL COMMENT '商品id',
  `notice` tinyint(1) DEFAULT '0' COMMENT '是否通知中奖，1是通知，0是尚未通知，默认为0',
  `order_id` varchar(255) DEFAULT NULL COMMENT '关联订单的号码',
  `order_num` int(11) unsigned DEFAULT NULL COMMENT '关联订单的购买数量',
  `logistics_company` varchar(255) DEFAULT NULL COMMENT '物流公司',
  `logistics_number` varchar(255) DEFAULT NULL COMMENT '物流单号',
  `logistics_status` tinyint(255) DEFAULT '0' COMMENT '获奖之后状态 1用户确认收货地址 2商品派发中 3商品派发中 4用户签收 5.晒单完成',
  `address_data` text COMMENT '地址信息 json快照',
  `qb_type` tinyint(3) DEFAULT '0' COMMENT '抢宝类型1:奇数2偶数',
  `luck_type` tinyint(3) DEFAULT '1' COMMENT '1商品中奖2香肠币中奖',
  `money` decimal(10,2) DEFAULT '0.00' COMMENT '中奖金额',
  `reward_type` tinyint(255) DEFAULT NULL COMMENT '0：实物商品：1，在线卡密；-1，直接返夺宝币',
  PRIMARY KEY (`id`),
  KEY `order_id` (`order_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='中奖记录表';

-- ----------------------------
-- Records of sp_win_record
-- ----------------------------

