INSERT INTO `sp_article` VALUES ('6', '公正保障', null, '112', '1', null, '\n                        <p>香肠一元购幸运号码计算公式，加入了“老时时彩”数据作为公正数据源，保证揭晓结果的随机性，保证绝对公正性。</p>\n                     ', null, null, null, null, null, null, null, null);
INSERT INTO `sp_article` VALUES ('7', '公开保障', null, '112', '1', null, '\n                        <p>香肠一元购的夺宝过程是完全透明的，您可以随时查看每个商品的参与人数、参与次数、参与名单及幸运号码信息等记录。</p>\n                     ', null, null, null, null, null, null, null, null);
INSERT INTO `sp_article` VALUES ('8', '安全支付', null, '112', '1', null, '\n                        <p style=\"margin-bottom:20px;\">香肠一元购使用支付宝、微信支付作为支付渠道。</p>\n                        <p style=\"margin-bottom:20px;\">方便用户进行网上交易推出的安全、稳定、快捷的在线支付。为用户提供了多种方便的在线充值、交易管理、在线支付、账户管理、提现等服务。</p>\n                        <p>支付宝、微信支付不仅减少每次用银行卡支付的繁琐流程，同时保护了您银行账户信息的安全，充分保证您在线支付的安全性。</p>\n                    ', null, null, null, null, null, null, null, null);
INSERT INTO `sp_article` VALUES ('9', '商品配送', null, '111', '1', null, '\n                        <p>在您成为幸运用户后，香肠一元购平台会第一时间为您寄出商品，大陆地区免快递费。</p>\n                        <p>获得商品后，请您尽快提交收货地址，超过7天未确认收货地址，视为自动放弃该商品。</p>\n                     ', null, null, null, null, null, null, null, null);
