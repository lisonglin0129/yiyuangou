-- ----------------------------
-- Table structure for sp_goods
-- ----------------------------
DROP TABLE IF EXISTS `sp_goods`;
CREATE TABLE `sp_goods` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `status` tinyint(2) DEFAULT '1' COMMENT '1正常 -1删除 -2禁用',
  `name` varchar(255) DEFAULT NULL COMMENT '商品标题',
  `sub_title` varchar(255) DEFAULT NULL COMMENT '副标题',
  `buy_times` int(11) DEFAULT '0' COMMENT '总购买人数',
  `category` int(11) DEFAULT NULL COMMENT '商品分类id',
  `mid` int(11) DEFAULT NULL COMMENT '所属大分类的id',
  `deposer_type` varchar(255) DEFAULT NULL COMMENT '夺宝类型,如果选择夺宝类型,则不允许用户修改预定义值',
  `init_times` int(11) DEFAULT NULL COMMENT '初始化购买次数,如果限制每次最少购买次数,则等于最少购买次数的倍数',
  `unit_price` decimal(10,2) DEFAULT NULL COMMENT '单价(计算得到)',
  `min_times` int(11) DEFAULT NULL COMMENT '每次最少购买次数',
  `sum_times` int(11) DEFAULT NULL COMMENT '购买总次数,由总金额除单价四舍五入多一位算得',
  `max_times` int(11) unsigned DEFAULT '1000' COMMENT '限制每次最大购买次数',
  `num` int(11) DEFAULT NULL COMMENT '库存数量',
  `index_img` int(255) unsigned DEFAULT NULL COMMENT '主图',
  `pic_list` text COMMENT '缩略图列表',
  `buy_price` decimal(10,2) DEFAULT NULL COMMENT '直接购买价格',
  `price` decimal(10,2) DEFAULT NULL COMMENT '一元购价格',
  `detail` text COMMENT '商品详情',
  `remarks` varchar(255) DEFAULT NULL COMMENT '备注',
  `parameter` tinytext COMMENT '产品参数地址或json数据',
  `search_times` int(11) DEFAULT '0' COMMENT '搜索次数，用于热门搜索',
  `update_time` int(11) DEFAULT NULL COMMENT '更新时间',
  `create_time` int(11) DEFAULT NULL COMMENT '创建时间',
  `shop_id` int(11) unsigned DEFAULT '0' COMMENT '商家id',
  `is_zero` tinyint(4) DEFAULT '0' COMMENT '是否参与0元抢宝 1 参与',
  `reward_type` tinyint(4) DEFAULT NULL COMMENT '0：实物商品；1：卡密；',
  `reward_data` int(255) unsigned DEFAULT NULL,
  `sec_open` tinyint(255) DEFAULT '0' COMMENT '是否秒开(1为是,0为否)',
  `is_packet` tinyint(1) DEFAULT '0' COMMENT '是否配置微信红包1配置0不配置',
  `packet_num` int(10) DEFAULT '0' COMMENT '微信红包领取人数',
  `packet_money` decimal(10,2) DEFAULT '0.00' COMMENT '红包金额',
  `is_cheat` enum('false','true') DEFAULT 'false' COMMENT '是否开启作弊模式  true开启  false关闭',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC COMMENT='商品基础信息表\r\n本表存放商品基础信息\r\n用于创建夺宝项目时选择使用';

