.main_image {
	width:100%;
	height:120px;
	border-top:1px solid #d7d7d7;
	margin:0 auto;
	position:relative
}
.main_image1s{border-top: none;}
.main_image ul {
	width:9999px;
	height:120px;
	overflow:hidden;
	position:absolute;
	top:0;
	left:0;
}
.main_image li {
	float:left;
	width:100%;
	height:120px;
}
.main_image li span {
	display:block;
	width:100%;
/*	height:120px;*/
}
.main_image li a {
	display:block;	
}
.main_image li a img{
	width:100%;
	height:49vw;
	overflow: hidden;
}
.main_image li .img_1 {
	background: url('../img/img_main_1.jpg') no-repeat 0 0;
	background-size:100% 100%;
}
.main_image li .img_2 {
	background: url('../img/img_main_2.jpg') no-repeat 0 0;
	background-size:100% 100%;
}
.main_image li .img_3 {
	background: url('../img/img_main_3.jpg') no-repeat 0 0;
	background-size:100% 100%;
	height: 49vw;
}
.main_image li .img_4 {
	background: url('../img/img_main_4.jpg') no-repeat 0 0;
	background-size:100% 100%;
}
.main_image li .img_5 {
	background: url('../img/img_main_5.jpg') no-repeat 0 0;
	background-size:100% 100%;
}
div.flicking_con {
	width:990px;
	margin:0 auto;
	position:relative
}
div.flicking_con .flicking_inner {
	position:absolute;
	top:360px;
	left:90px;
	z-index:999;
	width:300px;
	height:21px
} /* 121126 */
div.flicking_con a {
	float:left;
	width:21px;
	height:21px;
	margin:0;
	padding:0;
	background:url('../images/btn_main_img.png') 0 0 no-repeat;
	display:block;
	text-indent:-1000px
}
div.flicking_con a.on {
	background-position:0 -21px
}

